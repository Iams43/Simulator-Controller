[Cleanup]
Binaries Folder=%kBinariesDirectory%
Logs Folder=%kLogsDirectory%
Simulator Controller.ini=%kControllerConfigurationFile%
Simulator Tools.ini=%kToolsConfigurationFile%
*.bak Files=%kHomeDirectory%, *.ahk.bak, R
[Build]
Simulator Controller=%kBinariesDirectory%Simulator Controller.exe <- %kSourcesDirectory%Controller\Simulator Controller.ahk; %kIncludesDirectory%, %kPluginsDirectory%, %kUserPluginsDirectory%, %kHomeDirectory%VERSION
Simulator Settings=%kBinariesDirectory%Simulator Settings.exe <- %kSourcesDirectory%Startup\Simulator Settings.ahk; %kIncludesDirectory%, %kSourcesDirectory%Startup\Libraries\, %kHomeDirectory%VERSION
Simulator Startup=%kBinariesDirectory%Simulator Startup.exe <- %kSourcesDirectory%Startup\Simulator Startup.ahk; %kIncludesDirectory%, %kSourcesDirectory%Startup\Libraries\, %kHomeDirectory%VERSION
Simulator Configuration=%kBinariesDirectory%Simulator Configuration.exe <- %kSourcesDirectory%Tools\Simulator Configuration.ahk; %kIncludesDirectory%, %kHomeDirectory%VERSION
Simulator Tools=%kBinariesDirectory%Simulator Tools.exe <- %kSourcesDirectory%Tools\Simulator Tools.ahk; %kIncludesDirectory%, %kHomeDirectory%VERSION
;;;-------------------------------------------------------------------------;;;
;;;                            Initialization                               ;;;
;;;-------------------------------------------------------------------------;;;
										
[?Initialize] => (Prove: removeSettings("Assetto Corsa", "Sauber-Mercedes C9 1989",
										[Aero.Height.Front, Aero.Height.Rear, Aero.Wing.Front, Electronics.TC, Electronics.ABS])),
				 (Prove: addSettings("Assetto Corsa", "Sauber-Mercedes C9 1989",
									 [Aero.Height.Front.Left, Aero.Height.Front.Right, 
									  Aero.Height.Rear.Left, Aero.Height.Rear.Right,
									  Differential.Coast, Differential.Power,
									  Electronics.Turbo]))
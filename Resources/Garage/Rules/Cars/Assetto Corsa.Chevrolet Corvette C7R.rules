;;;-------------------------------------------------------------------------;;;
;;;                            Initialization                               ;;;
;;;-------------------------------------------------------------------------;;;

[?Initialize] => (Prove: removeSettings("Assetto Corsa", "Chevrolet Corvette C7R",
										[Electronics.ABS, Differential.Preload])),
				 (Prove: addSettings("Assetto Corsa", "Chevrolet Corvette C7R",
									 [Differential.Coast, Differential.Power]))
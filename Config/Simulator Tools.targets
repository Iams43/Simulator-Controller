[Cleanup]
Binaries Folder=%kBinariesDirectory%
Config Folder=%kConfigDirectory%
Logs Folder=%kLogsDirectory%
Simulator Controller.ini=%kControllerConfigurationFile%
Simulator Tools.ini=%kToolsConfigurationFile%
*.bak Files=%kHomeDirectory%, *.ahk.bak, Recursive
[Build]
Simulator Controller=%kBinariesDirectory%Simulator Controller.exe <- %kSourcesDirectory%Controller\Simulator Controller.ahk; %kIncludesDirectory%, %kSourcesDirectory%Controller\Plugins\
Simulator Configuration=%kBinariesDirectory%Simulator Configuration.exe <- %kSourcesDirectory%Startup\Simulator Configuration.ahk; %kIncludesDirectory%, %kSourcesDirectory%Startup\Libraries\
Simulator Startup=%kBinariesDirectory%Simulator Startup.exe <- %kSourcesDirectory%Startup\Simulator Startup.ahk; %kIncludesDirectory%, %kSourcesDirectory%Startup\Libraries\
Simulator Setup=%kBinariesDirectory%Simulator Setup.exe <- %kSourcesDirectory%Tools\Simulator Setup.ahk; %kIncludesDirectory%
Simulator Tools=%kBinariesDirectory%Simulator Tools.exe <- %kSourcesDirectory%Tools\Simulator Tools.ahk; %kIncludesDirectory%
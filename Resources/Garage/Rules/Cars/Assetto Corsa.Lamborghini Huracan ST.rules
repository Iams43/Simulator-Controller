;;;-------------------------------------------------------------------------;;;
;;;                            Initialization                               ;;;
;;;-------------------------------------------------------------------------;;;

[?Initialize] => (Prove: removeSettings("Assetto Corsa", "Lamborghini Huracan ST",
										[Aero.Wing.Front, Differential.Preload,
										 Bumpstop.Rate.Front.Left, Bumpstop.Rate.Front.Right,
										 Bumpstop.Rate.Rear.Left, Bumpstop.Rate.Rear.Right])),
				 (Prove: addSettings("Assetto Corsa", "Lamborghini Huracan ST",
									 [Differential.Coast, Differential.Power]))
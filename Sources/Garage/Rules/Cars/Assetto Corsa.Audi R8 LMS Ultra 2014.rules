;;;-------------------------------------------------------------------------;;;
;;;                            Initialization                               ;;;
;;;-------------------------------------------------------------------------;;;

[?Initialize] => (Prove: removeSettings("Assetto Corsa", "Audi R8 LMS Ultra 2014",
										[Differential.Preload,
										 Bumpstop.Rate.Front.Left, Bumpstop.Rate.Front.Right,
										 Bumpstop.Rate.Rear.Left, Bumpstop.Rate.Rear.Right])),
				 (Prove: addSettings("Assetto Corsa", "Audi R8 LMS Ultra 2014",
									 [Differential.Coast, Differential.Power]))
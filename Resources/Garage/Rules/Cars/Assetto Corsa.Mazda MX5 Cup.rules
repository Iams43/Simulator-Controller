;;;-------------------------------------------------------------------------;;;
;;;                            Initialization                               ;;;
;;;-------------------------------------------------------------------------;;;
										
[?Initialize] =>    (Prove: removeSettings("Assetto Corsa", "Mazda MX5 Cup",
										   [Aero.Height.Front, Aero.Height.Rear, Aero.Wing.Front, Aero.Wing.Rear, Brake.Balance,
										   Bumpstop.Rate.Front.Left, Bumpstop.Rate.Front.Right, Bumpstop.Rate.Rear.Left, Bumpstop.Rate.Rear.Right,
										   Bumpstop.Range.Front.Left, Bumpstop.Range.Front.Right, Bumpstop.Range.Rear.Left, Bumpstop.Range.Rear.Right,
										   Damper.Compression.Fast.Front.Left, Damper.Compression.Fast.Front.Right, Damper.Compression.Fast.Rear.Left, Damper.Compression.Fast.Rear.Right,
										   Damper.Rebound.Fast.Front.Left, Damper.Rebound.Fast.Front.Right, Damper.Rebound.Fast.Rear.Left, Damper.Rebound.Fast.Rear.Right,
										   Differential.Preload, Differential.Power, Differential.Coast])),
					(Prove: addSettings("Assetto Corsa", "Mazda MX5 Cup",
										[Aero.Height.Front.Left, Aero.Height.Front.Right, 
										 Aero.Height.Rear.Left, Aero.Height.Rear.Right,
										 Electronics.TC, Electronics.ABS]))
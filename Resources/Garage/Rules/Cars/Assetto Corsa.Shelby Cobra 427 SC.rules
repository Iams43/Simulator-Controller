;;;-------------------------------------------------------------------------;;;
;;;                            Initialization                               ;;;
;;;-------------------------------------------------------------------------;;;

[?Initialize] => (Prove: removeSettings("Assetto Corsa", "Shelby Cobra 427 SC",
										[Electronics.TC, Electronics.ABS, Differential.Preload,
										 Aero.Wing.Front, Aero.Wing.Rear,
										 Bumpstop.Rate.Front.Left, Bumpstop.Rate.Front.Right,
										 Bumpstop.Rate.Rear.Left, Bumpstop.Rate.Rear.Right,
										 Bumpstop.Range.Front.Left, Bumpstop.Range.Front.Right,
										 Bumpstop.Range.Rear.Left, Bumpstop.Range.Rear.Right])),
				 (Prove: addSettings("Assetto Corsa", "Shelby Cobra 427 SC",
									 [Differential.Coast, Differential.Power]))
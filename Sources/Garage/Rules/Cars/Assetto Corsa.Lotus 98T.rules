;;;-------------------------------------------------------------------------;;;
;;;                            Initialization                               ;;;
;;;-------------------------------------------------------------------------;;;

[?Initialize] => (Prove: removeSettings("Assetto Corsa", "Lotus 98T",
										[Electronics.TC, Electronics.ABS])),
				 (Prove: addSettings("Assetto Corsa", "Lotus 98T",
									 [Electronics.Turbo, Electronics.Engine.Limiter, Differential.Coast, Differential.Power]))
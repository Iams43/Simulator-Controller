[?Initialize] => (Prove: removeSettings("Assetto Corsa", "BMW M3 GT2",
										[Electronics.ABS, Differential.Preload])),
				 (Prove: addSettings("Assetto Corsa", "BMW M3 GT2",
									 [Differential.Coast, Differential.Power]))